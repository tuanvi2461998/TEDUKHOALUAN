@model TeduCoreApp.Models.ProductViewModels.OrderViewModels
@{
    ViewData["Title"] = "Index";
    var order = Model.Biill;
}
<!-- Main Container -->
<section class="main-container col2-right-layout">
    <div class="main container">
        <div class="row">
            <div class="col-main col-sm-9 col-xs-12">
                <div class="my-account">
                    <div class="page-title">
                        <h2>Orders List</h2>
                    </div>
                    <div class="orders-list table-responsive">
                        <!--orders list table-->
                        <table class="table table-bordered cart_summary table-striped">
                            <thead>
                                <tr>
                                    <!--titles for td-->
                                    <th>Số hóa đơn</th>
                                    <th>Ngày đặt</th>
                                    <th>Trạng thái</th>
                                    <th>Tổng tiền</th>
                                </tr>
                            </thead>
                            <tbody>

                                <tr>
                                    <!--order number-->
                                    <td data-title="@order."><a href="order_details.html" class="order-number">#12345</a></td>
                                    <!--order date-->
                                    <td data-title="Order Date">2017-02-13</td>
                                    <!--order status-->
                                    <td data-title="Order Status">Confirmed by ShopMart</td>
                                    <!--quanity-->
                                    <td data-title="Total"><span class="order-total">$79.00</span></td>
                                </tr>
                               
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>